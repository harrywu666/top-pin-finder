{
  "pinterest": {
    "login_required": false,
    "session_cookie": ""
  },
  "search": {
    "keywords": "landscape",
    "min_likes": 50,
    "max_results": 5,
    "scroll_delay": 2
  },
  "download": {
    "save_path": "./downloads",
    "naming_format": "{category}_{likes}_{index}",
    "image_format": "jpg",
    "min_resolution": {
      "width": 800,
      "height": 600
    }
  },
  "behavior": {
    "random_delay_min": 1,
    "random_delay_max": 3,
    "user_agents": [
      "Mozilla/5.0 (Windows NT 10.0; Win64; x64) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/120.0.0.0 Safari/537.36"
    ]
  },
  "logging": {
    "level": "INFO",
    "file": "pinterest_downloader.log"
  }
}